#根据URL获取方法名称
exports.getCmdName = ( url )->
	reg = /(.*\/)(.*)(\.cmd)/
	rlt = reg.exec( url )
	return rlt[2]

#根据URL获取方法名称
exports.getMethodName = ( url )->
	reg = /(method=)(.*)(\&?)/
	rlt = reg.exec( url )
	return rlt[2]

#第一个字母大写
exports.transferFirstLetterUpper = ( cmdName )->
	return cmdName[0...1].toLocaleUpperCase() + cmdName[1..]